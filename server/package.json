{
  "name": "2weiwall-server",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\""
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "nodemon": "^1.19.3",
    "typescript": "^3.6.3"
  }
}
